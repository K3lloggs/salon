{
  "cli": {
    "version": ">= 15.0.14",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "extraPodfileProperties": {
          "use_modular_headers!": true,
          "use_frameworks!": "static"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m-medium",
        "extraPodfileProperties": {
          "use_modular_headers!": true,
          "use_frameworks!": "static"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}